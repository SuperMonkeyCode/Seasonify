{
  "name": "Seasonify",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "adb reverse tcp:8081 tcp:8081 && rndebugger-open && node node_modules/react-native/local-cli/cli.js start",
    "android": "cd android && gradlew installDebug && adb shell am start -n com.seasonify/.MainActivity",
    "date": "adb shell \"date `date +%m%d%H%M%Y.%S`\"",
    "lint": "eslint ./",
    "test": "SET NODE_ENV=testing && jest",
    "watch": "SET NODE_ENV=testing && jest --watch",
    "coverage": "SET NODE_ENV=testing && jest --coverage",
    "build:ios": "bundle install --binstubs ./examples/ios && bundle exec pod install --project-directory=./example/ios/",
    "build:android": "cd android && gradlew :react-native-maps:assembleDebug",
    "clean": "rm -rf $TMPDIR/react-* && npm cache clean && node node_modules/react-native/local-cli/cli.js start --reset-cache"
  },
  "dependencies": {
    "axios": "~0.16.1",
    "babel-preset-react-native-stage-0": "^1.0.1",
    "jshashes": "^1.0.6",
    "native-base": "^2.1.3",
    "react": "16.0.0-alpha.6",
    "react-native": "0.44",
    "react-native-fit-image": "^1.4.8",
    "react-navigation": "^1.0.0-beta.9",
    "react-redux": "^5.0.5",
    "redux": "^3.6.0",
    "redux-devtools-extension": "^2.13.2",
    "redux-persist": "^4.8.0",
    "redux-saga": "^0.15.3",
    "throttle-debounce": "^1.0.1",
    "uuid": "^3.0.1"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "babel-jest": "^20.0.0",
    "babel-preset-react-native": "^1.9.1",
    "deep-freeze": "0.0.1",
    "enzyme": "^2.8.2",
    "enzyme-to-json": "^1.5.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-import-resolver-reactnative": "^1.0.2",
    "eslint-plugin-flowtype": "^2.34.0",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-react": "^7.0.1",
    "jest": "^20.0.0",
    "jest-cli": "^20.0.3",
    "react-dom": "^15.5.4",
    "react-native-debugger-open": "^0.3.11",
    "react-test-renderer": "^15.5.4",
    "reactotron-react-native": "^1.10.0",
    "reactotron-redux": "^1.10.0",
    "reactotron-redux-saga": "^1.10.0",
    "redux-saga-test-plan": "^3.1.0",
    "why-did-you-update": "0.0.8"
  },
  "jest": {
    "preset": "react-native",
    "setupFiles": [
      "./jest.setup.js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "transformIgnorePatterns": [
      "/node_modules/(?!native-base)/"
    ]
  }
}
