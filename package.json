{
  "name": "Seasonify",
  "version": "0.0.1",
  "private": true,
  "moduleRoots": [
    "app/"
  ],
  "scripts": {
    "lint": "eslint ./app",
    "flow": "flow || exit 0",
    "t": "cross-env NODE_ENV=development jest -o --coverage",
    "test": "cross-env NODE_ENV=development jest --coverage",
    "watch": "cross-env NODE_ENV=development jest --watch",
    "coverage": "cross-env NODE_ENV=development CI=true jest --coverage",
    "start": "npm run adb && npm run adb:reactotron && rndebugger-open && node node_modules/react-native/local-cli/cli.js start",
    "haul": "npm run adb:reactotron && rndebugger-open && haul start --platform android",
    "android": "cd android && gradlew installDebug && adb shell am start -n com.seasonify/.MainActivity",
    "adb": "adb reverse tcp:8081 tcp:8081",
    "adb:reactotron": "adb reverse tcp:9090 tcp:9090",
    "adb:date": "adb shell \"date `date +%m%d%H%M%Y.%S`\"",
    "build:ios": "bundle install --binstubs ./examples/ios && bundle exec pod install --project-directory=./example/ios/",
    "build:android": "cd android && gradlew :react-native-maps:assembleDebug",
    "clean": "rm -rf $TMPDIR/react-* && npm cache clean && node node_modules/react-native/local-cli/cli.js start --reset-cache",
    "precommit": "echo Flow... && flow --quiet && lint-staged",
    "prepush": "cross-env NODE_ENV=development jest --silent --coverage && echo Eslint... && eslint --quiet --max-warnings 10 ./app && echo Flow... && flow --quiet"
  },
  "lint-staged": {
    "*.js": []
  },
  "lint-staged-BACKUP": {
    "*.js": [
      "prettier-eslint --write",
      "git add",
      "eslint --quiet --max-warnings 10"
    ]
  },
  "dependencies": {
    "axios": "^0.16.2",
    "babel-plugin-module-resolver": "^2.7.1",
    "babel-preset-react-native-stage-0": "^1.0.1",
    "jshashes": "^1.0.7",
    "native-base": "^2.2.0",
    "react": "^16.0.0-alpha.6",
    "react-native": "^0.44.3",
    "react-native-color-picker": "^0.3.0",
    "react-navigation": "^1.0.0-beta.11",
    "react-redux": "^5.0.5",
    "recompose": "^0.24.0",
    "redux": "^3.7.2",
    "redux-devtools-extension": "^2.13.2",
    "redux-persist": "^4.8.2",
    "redux-persist-migrate": "^4.1.0",
    "redux-saga": "^0.15.4",
    "reduxsauce": "^0.6.0",
    "throttle-debounce": "^1.0.1",
    "uuid": "^3.1.0"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "babel-jest": "^20.0.3",
    "babel-preset-react-native": "^2.1.0",
    "cross-env": "^5.0.1",
    "deep-diff": "^0.3.8",
    "deep-freeze": "0.0.1",
    "enzyme": "^2.9.1",
    "enzyme-to-json": "^1.5.1",
    "eslint": "^4.2.0",
    "eslint-config-airbnb": "^15.0.2",
    "eslint-import-resolver-reactnative": "^1.0.2",
    "eslint-plugin-flowtype": "^2.35.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.1.0",
    "eslint-plugin-react-native": "^2.3.2",
    "flow-bin": "^0.50.0",
    "haul": "^1.0.0-beta.2",
    "husky": "^0.14.3",
    "jest": "^20.0.4",
    "jest-cli": "^20.0.4",
    "lint-staged": "^4.0.2",
    "prettier-eslint": "^6.4.2",
    "react-addons-perf": "^15.4.2",
    "react-dom": "^15.6.1",
    "react-native-debugger-open": "^0.3.11",
    "react-test-renderer": "^15.6.1",
    "reactotron-react-native": "^1.12.1",
    "reactotron-redux": "^1.12.0",
    "reactotron-redux-saga": "^1.12.0",
    "redux-saga-test-plan": "^3.1.0",
    "reselect": "^3.0.1"
  }
}
